<?xml version="1.0" encoding="UTF-8"?>
<templateSet group="ColdFusion">
  <template name="cfs" value="&lt;cfset $VAR$ = $VALUE$/&gt;"
            resource-bundle="messages.CfmlBundle" key="livetemplate.description.cfs" toReformat="false"
            toShortenFQNames="true">
    <variable name="VAR" expression="&quot;var&quot;" defaultValue="&quot;var&quot;" alwaysStopAt="true"/>
    <variable name="VALUE" expression="&quot;value&quot;" defaultValue="&quot;value&quot;" alwaysStopAt="true"/>
    <context>
      <option name="ColdFusion" value="true" />
    </context>
  </template>

